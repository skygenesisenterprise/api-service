{
  "name": "api-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": " pnpm run dev:frontend && pnpm run dev:backend",
    "dev:backend": "tsx api/src/server.ts",
    "dev:frontend": "next dev",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "next build",
    "build:backend": "tsc --project api/tsconfig.json",
    "start": "next start",
    "start:backend": "node api/dist/server.js",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "typecheck:backend": "tsc --noEmit --project api/tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate --schema prisma/schema.prisma",
    "db:push": "prisma db push --schema prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema prisma/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema prisma/schema.prisma",
    "db:migrate:reset": "prisma migrate reset --schema prisma/schema.prisma",
    "db:studio": "prisma studio --schema prisma/schema.prisma",
    "db:seed:demo": "tsx prisma/seed-demo-user.ts",
    "db:format": "prisma format --schema prisma/schema.prisma",
    "db:validate": "prisma validate --schema prisma/schema.prisma"
  },
  "dependencies": {
